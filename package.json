{
  "name": "pb-mathcat-node-wrapper",
  "version": "1.0.0",
  "description": "Node MathCAT package made with napi-rs for Pressbooks math microservice",
  "main": "index.js",
  "license": "GPL-3.0-only",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "binaryName": "pb-mathcat-node-wrapper",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu"    ]
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:linux-arm64": "napi build --platform --release --target aarch64-unknown-linux-gnu --use-cross",
    "test": "node test/index.test.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.3.1"
  }
}
